/* 
  Theme: "Horizonte de Oportunidades" (Clean & Modern)
  Primary: Turquoise Blue (#00A896) -> oklch(0.65 0.15 180) approx
  Accent: Solar Yellow (#F9C74F) -> oklch(0.85 0.18 85) approx
  Background: White / Light Gray
*/

:root {
  /* Primary: #00A896 (Turquoise) */
  --primary: oklch(0.62 0.16 185); 
  --primary-foreground: oklch(0.98 0 0);
  
  /* Sidebar Primary matches Primary */
  --sidebar-primary: oklch(0.62 0.16 185);
  --sidebar-primary-foreground: oklch(0.98 0 0);

  /* Chart colors based on turquoise/teal spectrum */
  --chart-1: oklch(0.62 0.16 185);
  --chart-2: oklch(0.55 0.15 190);
  --chart-3: oklch(0.48 0.14 195);
  --chart-4: oklch(0.85 0.18 85); /* Accent yellow for contrast */
  --chart-5: oklch(0.40 0.10 200);

  --radius: 0.75rem; /* Soft rounded corners */

  /* Backgrounds */
  --background: oklch(1 0 0);
  --foreground: oklch(0.25 0.02 260); /* Dark Gray #1F2937 */

  --card: oklch(1 0 0);
  --card-foreground: oklch(0.25 0.02 260);

  --popover: oklch(1 0 0);
  --popover-foreground: oklch(0.25 0.02 260);

