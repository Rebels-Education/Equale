  /* Accent: Solar Yellow #F9C74F */
  --accent: oklch(0.97 0.01 260); /* Subtle hover background */
  --accent-foreground: oklch(0.62 0.16 185);

  --destructive: oklch(0.577 0.245 27.325);
  --destructive-foreground: oklch(0.985 0 0);

  --border: oklch(0.92 0.01 260);
